
public class NthPrime {
	
	static int count=0;
	static int i=1;
	
	public static boolean isPrime(int number) {
	      if (number == 1)
	            return false;
	      if (number == 2)
	            return true;
	      if (number % 2 == 0)
	            return false;
	      for (int d = 3; d <= (int) Math.sqrt(number); d++)
	            if (number % d == 0)
	                  return false;
	      return true;
	}
	public static void main(String[] args) {
		while(true){
			i++;
			if(isPrime(i)){
				count++;				
			}
			if(count==2){
				System.out.println(i);
				break;
			}	
		}
	}
}
